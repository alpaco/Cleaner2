<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background" android:top="2dp" android:bottom="2dp">
    <shape>
        <size
            android:height="2dp"/>
        <solid
            android:color="#00F09819" />
        <corners
            android:radius="10dp"/>
    </shape>
</item>

<item
android:id="@android:id/progress">
<clip>
    <shape>
        <size
            android:height="2dp"/>
        <gradient
            android:startColor="@color/gradient_blue_start"
            android:endColor="@color/gradient_blue_end" />
        <corners
            android:radius="16dp"/>
    </shape>
</clip>
</item>
</layer-list>