<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background" android:top="2dp" android:bottom="2dp">
    <shape>
        <size
            android:height="5dp"/>
        <solid
            android:color="@color/gradient_orange_end" />
    </shape>
</item>

<item
android:id="@android:id/progress">
<clip>
    <shape>
        <size
            android:height="10dp"/>
        <solid
            android:color="@color/gradient_blue_end" />
        <corners
            android:radius="10dp"/>
    </shape>
</clip>
</item>
</layer-list>